{
    "env": {
        "es6": true,
        "node": true,
        "qunit": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "no-console" : [ 0 ],
        "indent": [
            "error",
            4
        ],
        "keyword-spacing": "error",
        "linebreak-style": 0,
        "quotes": [
            "error",
            "double"
        ],
        "semi": [
            "error",
            "always"
        ]
    }
}